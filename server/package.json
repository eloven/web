        "mongoose": "^5.3.16",
        "multer": "^1.4.1",
        "nodemon": "^1.18.8",
        "request": "^2.88.0",
        "request-promise": "^4.2.2",
        "shelljs": "^0.8.3"
    },
    "devDependencies": {
        "@types/express-session": "^1.15.11",
        "@types/cookie-parser": "^1.4.1",
